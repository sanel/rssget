SubDir TOP test ;

rule TestMain {
  ObjectCcFlags $(>) : $(GLOBAL_CFLAGS) ;

  Main $(<) : $(>) ;
  LINKLIBS on $(<) = [ on $(<) return $(LINKLIBS) ] $(RSSGET_LIB_FLAGS) ;

  LocalDepends all : $(<) ;
  LinkLibraries $(<) : $(RSSGET_LIB) ;
}


TestMain tap-test : tap-test.c ;
TestMain utils-test : utils-test.c ;